{
    "terminal.integrated.shell.osx": "/bin/zsh",
    "terminal.integrated.shellArgs.osx": [],
    "terminal.integrated.persistentSessionReviveProcess": "never",
    "terminal.integrated.enablePersistentSessions": false,
    "terminal.integrated.scrollback": 10000,
    "terminal.integrated.commandsToSkipShell": [
        "workbench.action.terminal.focus",
        "workbench.action.terminal.new",
        "workbench.action.terminal.kill"
    ],
    "terminal.integrated.env.osx": {
        "PATH": "/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/opt/homebrew/bin"
    },
    "terminal.integrated.automationProfile.osx": {
        "path": "/bin/zsh",
        "args": [],
        "env": {
            "PATH": "/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:/opt/homebrew/bin"
        }
    },
    "editor.formatOnSave": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit",
        "source.fixAll.stylelint": "explicit"
    },
    "[swift]": {
        "editor.formatOnSave": false,
        "editor.codeActionsOnSave": {}
    },
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
        "**/.build": true,
        "**/.swiftpm": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/.build": true,
        "**/.swiftpm": true,
        "**/*.log": true
    },
    "swift.buildPath": ".build",
    "swift.toolchainPath": null,
    "swift.backgroundCompilation": false,
    "swift.showBuildStatus": "never",
    "swift.format.enable": true,
    "swift.lint.enable": true,
    "swift.lint.onSave": false,
    "swift.format.onSave": false,
    "swift.path.swiftlint": "swiftlint",
    "swift.path.swiftformat": "swiftformat",
    "python.terminal.activateEnvironment": false,
    "python.defaultInterpreterPath": "/usr/bin/python3",
    "extensions.ignoreRecommendations": true,
    "workbench.editor.enablePreview": false,
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.value": 10,
    "workbench.editor.limit.perEditorGroup": true,
    "git.autofetch": false,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "git.postCommitCommand": "none"
}